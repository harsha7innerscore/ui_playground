{
  "name": "playwright-automation-framework",
  "version": "1.0.0",
  "description": "Automate end-to-end testing for web applications using Playwright with TypeScript",
  "main": "index.js",
  "engines": {
    "node": ">=18.20.4"
  },
  "scripts": {
    "test": "playwright test",
    "test:smoke": "playwright test tests/smoke",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:headed": "playwright test --headed",
    "test:report": "playwright show-report",
    "test:trace": "playwright show-trace"
  },
  "keywords": [
    "playwright",
    "automation",
    "testing",
    "e2e",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/node": "^25.0.6",
    "playwright": "^1.57.0",
    "typescript": "^5.9.3"
  }
}
